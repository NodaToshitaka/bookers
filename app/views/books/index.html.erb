<h1>Books</h1>
<% @books.each do |list| %>
 <p><%= book.title %></p>
 <p><%= book.body %></p>
 <%= link_to "Show", book_path(@book:id) %>
 <%= link_to "Edit", edit_book_path(@book:id) %>
 <%= link_to "Destroy", destroy_book_path(@book:id) %>
<% end %>
<h2>New book</h2>
 <%= form_with model:@book, url:'/books', local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Create Book' %>
 <% end %>